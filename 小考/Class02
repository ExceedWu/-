class test{
	private int a = 0;
	int b = 0;
	public void setX(int x) {
		for (int i=1;i<x;i++) {
			if (x%i==0) {
				b=b+1;
			}
		}
		if(b==1) {
			System.out.println("Yes");
		}
		else {
			System.out.println("No");
		}
	}
}

public class Hi {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		test a = new test();
		a.setX(39);
		
	}

}
